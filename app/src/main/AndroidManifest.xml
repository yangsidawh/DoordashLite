<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sida.doordashlite">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/doordash"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/doordash"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.StoreListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.StoreDetailActivity"
            android:label="@string/title_activity_store_detail"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>